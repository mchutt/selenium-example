<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Automation Website" verbose="1" parallel="tests" thread-count="2">

    <test name="Chrome" >
        <parameter name="browser-name" value="chrome" />
        <classes>
            <class name="com.solvd.example.web.automationwebpage.AutomationTest" />
        </classes>
    </test>
    <test name="Firefox" >
        <parameter name="browser-name" value="firefox" />
        <classes>
            <class name="com.solvd.example.web.automationwebpage.AutomationTest" />
        </classes>
    </test>
</suite>